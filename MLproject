name: sistem-diabetes-classifier

conda_env: conda.yaml

entry_points:
  main: 
    parameters:
      n_estimators: {type: int, default: 200}
      max_depth: {type: int, default: 20}
      dataset : {type: string, default: 'https://drive.google.com/file/d/1i5MwJku1raP2AaRqD5DITomIr7kSA3qo/view?usp=sharing'}
    command: "python modelling.py {n_estimators} {max_depth} {dataset}"